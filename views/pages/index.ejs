                </tr>
              </table>
            </div>
          </td>
        </tr>

        <!-- Squads -->
        <tr>
          <td class="collapsible-section-header" colspan="4">
            <a data-toggle="collapse" href="#collapse_section_three">Squads</a>
          </td>
        </tr>
        <tr>
          <td class="no-padding" colspan="4">
            <div id="collapse_section_three" class="collapse in">
              <table style="width:100%">
                <tr>
                  <td><%= stats[0].stats.p9.top1.value%></td>
                  <td><%= stats[1].stats.p9.top1.value%></td>
                  <td><%= stats[2].stats.p9.top1.value%></td>
                  <td><%= stats[3].stats.p9.top1.value%></td>
                </tr>
                <tr>
